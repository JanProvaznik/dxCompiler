{
    "cwlVersion": "v1.2",
    "$namespaces": {
        "edam": "http://edamontology.org/",
        "gx": "http://galaxyproject.org/formats/"
    },
    "$schemas": [
        "dx://project-Fy9QqgQ0yzZbg9KXKP4Jz6Yq:/test_data/cwl/EDAM.owl",
        "https://raw.githubusercontent.com/broadinstitute/cromwell/develop/cwl/src/test/resources/cwl/ontology/gx_edam.ttl"
    ],
    "$graph": [
        {
            "id": "main",
            "class": "CommandLineTool",
            "requirements": [
                {
                    "class": "NetworkAccess",
                    "networkAccess": true
                },
                {
                    "class": "InlineJavascriptRequirement"
                }
            ],
            "cwlVersion": "v1.2",
            "inputs": [
                {
                    "id": "reference",
                    "type": "File",
                    "format": "gx:fasta",
                    "inputBinding": {
                        "position": 2
                    }
                }
            ],
            "outputs": [
                {
                    "id": "lineCount",
                    "type": "int",
                    "outputBinding": {
                        "glob": "stdout",
                        "loadContents": true,
                        "outputEval": "$(parseInt(self[0].contents))"
                    }
                }
            ],
            "baseCommand": [
                "wc",
                "-l"
            ],
            "stdout": "stdout",
            "hints": {
                "LoadListingRequirement": {
                    "loadListing": "deep_listing"
                }
            }
        }
    ]
}