{
    "class": "CommandLineTool",
    "inputs": [
        {
            "type": "Any",
            "inputBinding": {},
            "id": "#main/in"
        }
    ],
    "baseCommand": "echo",
    "stdout": "out.txt",
    "id": "#main",
    "outputs": [
        {
            "type": "string",
            "outputBinding": {
                "glob": "out.txt",
                "loadContents": true,
                "outputEval": "$(self[0].contents)"
            },
            "id": "#main/out"
        }
    ],
    "cwlVersion": "v1.2"
}