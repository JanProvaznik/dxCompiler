            "valueFrom": "${\n  var r = [];\n  for (var i = 10; i >= 1; i--) {\n    r.push(i);\n  }\n  return r;\n}\n"
        },
        {
            "prefix": "-D",
            "valueFrom": "$(true)"
        },
        {
            "prefix": "-E",
            "valueFrom": "$(false)"
        }
    ],
    "id": "#main",
    "cwlVersion": "v1.2"
}